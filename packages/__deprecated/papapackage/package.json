{
  "name": "papapackage",
  "version": "1.0.38",
  "type": "module",
  "description": "",
  "main": "./build/tsc/cli.js",
  "types": "./build/tsc/cli.d.ts",
  "bin": {
    "papapackage": "./build/tsc/cli.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Code-From-Anywhere/papapackage.git"
  },
  "author": "Wijnand Karsens",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Code-From-Anywhere/papapackage/issues"
  },
  "homepage": "https://github.com/Code-From-Anywhere/papapackage#readme",
  "scripts": {
    "pub": "npm version patch --force && npm publish",
    "cli-comment": "These extra parameters are absolutely required.  wont work without them.",
    "cli": "node --loader ts-node/esm --experimental-specifier-resolution=node src/cli.ts",
    "lint": "eslint src/ --ext .js,.jsx,.ts,.tsx",
    "test": "jest",
    "clean": "rm -rf build package",
    "ts-node": "ts-node",
    "docs": "typedoc --entryPoints src/cli.ts",
    "build": "tsc -p tsconfig.json",
    "build-all": "yarn clean && yarn build && yarn esbuild-node",
    "esbuild-node": "esbuild src/cli.ts --bundle --platform=node --minify --sourcemap=external --external:node:readline --external:node:process --outfile=build/esbuild/cli.js",
    "esbuild-node:dev": "esbuild src/cli.ts --bundle --platform=node --sourcemap=external --external:node:readline --external:node:process --outfile=build/esbuild/cli.js",
    "esbuild-node:watch": "esbuild src/cli.ts --bundle --platform=node --watch --sourcemap=external --external:node:readline --external:node:process --outfile=build/esbuild/cli.js"
  },
  "devDependencies": {
    "@types/jest": "^26.0.24",
    "@typescript-eslint/eslint-plugin": "^5.5.0",
    "@typescript-eslint/parser": "^5.5.0",
    "esbuild": "^0.11.11",
    "eslint": "^7.22.0",
    "jest": "^27.4.3",
    "ts-jest": "^27.0.7",
    "typedoc": "^0.20.35",
    "@types/fb-watchman": "^2.0.1",
    "@types/node": "^16.11.12",
    "@types/yargs": "^17.0.7",
    "babel-cli": "^6.26.0",
    "nodemon": "^2.0.15",
    "pkg": "^5.5.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.3"
  },
  "dependencies": {
    "colors": "^1.4.0",
    "fb-watchman": "^2.0.1",
    "ora": "^6.0.1",
    "yargs": "^17.3.0"
  }
}
